{
    "presets": [
        "@babel/preset-env",
        [
            "@babel/preset-react",
            {
                "targets": {
                    "ie": "11"
                },
                "useBuiltIns": "entry",
                "corejs": 3,
                "runtime": "automatic"
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "regenerator": true
            }
        ]
    ]
}